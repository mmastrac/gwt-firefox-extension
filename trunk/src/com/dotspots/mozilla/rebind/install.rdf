<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>@lguid</em:id>
		<em:name>@name</em:name>
		<em:version>@version</em:version>
		<em:description>@description</em:description>
		<em:creator>DotSpots</em:creator>
		
		<!--
		<em:homepageURL></em:homepageURL>
		<em:iconURL></em:iconURL>
		<em:aboutURL></em:aboutURL>
		<em:optionsURL></em:optionsURL>
		<em:updateURL></em:updateURL>
		-->
		
		<em:type>2</em:type>
		
		<em:file>
			<Description
				about="urn:mozilla:extension:@lguid">
				<em:package>content/@lguid/</em:package>
				<em:skin>skin/classic/@lguid/</em:skin>
			</Description>
		</em:file>

		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>4.0</em:maxVersion>
			</Description>
		</em:targetApplication>

		<!-- Flock -->
		<em:targetApplication>
			<Description>
				<em:id>{a463f10c-3994-11da-9945-000d60ca027b}</em:id>
				<em:minVersion>0.7</em:minVersion>
				<em:maxVersion>1.0</em:maxVersion>
			</Description>
		</em:targetApplication>

		<!-- Fennec -->
		<em:targetApplication>
			<Description>
				<em:id>{a23983c0-fd0e-11dc-95ff-0800200c9a66}</em:id>
				<em:minVersion>0.1</em:minVersion>
				<em:maxVersion>1.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>

</RDF>
